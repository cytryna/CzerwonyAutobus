<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<AnchorPane  id="tabbedDisplayAnchor" maxHeight="-1.0" maxWidth="-1.0"
            prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" 
            xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" 
            fx:controller="com.diligentia.controllers.TabPaneManger">
    <children>
        <TabPane fx:id="tabPane" prefHeight="368.0" prefWidth="640.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <tabs>
                <Tab fx:id="menuTab" text="Console">
                    <fx:include source="MenuTab.fxml"/>
                </Tab>
                <Tab fx:id="mealTab" text="Meal">
                    <fx:include  source="MealTab.fxml"/>
                </Tab>
                <Tab fx:id="loggerTab" text="Logger">
                    <fx:include source="LoggerTab.fxml"/>
                </Tab>
            </tabs>
        </TabPane>
    </children>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
</AnchorPane>
